generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Person {
  id         String   @id @default(uuid())
  apelido    String   @unique @db.VarChar(32)
  nome       String   @db.VarChar(100)
  nascimento DateTime
  stack      String?

  @@map("people")
}
