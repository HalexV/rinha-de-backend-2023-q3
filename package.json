{
  "name": "nest-app-rinha-backend-2023",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/infra/main",
    "start:migrate:prod": "prisma migrate deploy && npm run start:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest --reporter='verbose'",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "pretest:e2e": "docker compose -f docker-compose-test-e2e.yml up -d",
    "test:e2e": "vitest --config ./vitest.config.e2e.ts || docker compose -f docker-compose-test-e2e.yml down",
    "posttest:e2e": "docker compose -f docker-compose-test-e2e.yml down",
    "clinic:doctor": "npx clinic doctor -- node dist/src/infra/main",
    "clinic:flame": "npx clinic flame -- node dist/src/infra/main",
    "clinic:heapprofiler": "npx clinic heapprofiler -- node dist/src/infra/main",
    "clinic:bubbleprof": "npx clinic bubbleprof --collect-only -- node dist/src/infra/main"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-fastify": "^10.2.4",
    "@prisma/client": "^5.2.0",
    "fastify": "^4.22.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "zod": "^3.22.2",
    "zod-validation-error": "^1.5.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/core": "^1.3.82",
    "@types/node": "^20.3.1",
    "@types/pg": "^8.10.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^0.34.3",
    "clinic": "^13.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "prisma": "^5.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "unplugin-swc": "^1.4.2",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.3"
  }
}
